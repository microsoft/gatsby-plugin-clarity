{
  "name": "gatsby-plugin-clarity",
  "version": "1.1.0",
  "description": "Plugin to add the tracking code for microsoft.clarity.com",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "analytics",
    "microsoft-clarity",
    "heatmap",
    "session recording"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/microsoft/gatsby-plugin-clarity/issues"
  },
  "homepage": "https://github.com/microsoft/gatsby-plugin-clarity#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/gatsby-plugin-clarity"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "babel-preset-gatsby-package": "^1.2.0-next.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
